{
  "serviceName": "multilingual-inference-hybrid",
  "cluster": "multilingual-inference-cluster",
  "taskDefinition": "multilingual-inference-hybrid",
  "desiredCount": 1,
  "launchType": "FARGATE",
  "platformVersion": "LATEST",
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": [
        "subnet-XXXXXXXXX",
        "subnet-YYYYYYYYY"
      ],
      "securityGroups": [
        "sg-XXXXXXXXX"
      ],
      "assignPublicIp": "ENABLED"
    }
  },

  "deploymentConfiguration": {
    "maximumPercent": 200,
    "minimumHealthyPercent": 50,
    "deploymentCircuitBreaker": {
      "enable": true,
      "rollback": true
    }
  },
  "enableExecuteCommand": true,
  "tags": [
    {
      "key": "Environment",
      "value": "production"
    },
    {
      "key": "Service",
      "value": "hybrid"
    },
    {
      "key": "Platform",
      "value": "ARM64"
    }
  ]
}